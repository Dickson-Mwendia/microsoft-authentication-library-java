{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-autogen",
        "dest": "api",
        "group": "latest",
        "version": "msal-java-latest",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/toc.yml"
        ]
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "breadcrumb",
        "dest": "breadcrumb",
        "group": "latest",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/toc.yml"
        ],
        "src": "docs-ref-autogen",
        "dest": "api/msal-java",
        "group": "latest",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-conceptual",
        "dest": "api/overview/msal-java",
        "group": "latest"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "src": "docs-ref-conceptual",
        "group": "latest"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "breadcrumb/toc.yml",
      "ms.topic": "managed-reference",
      "ms.devlang": "java",
      "apiPlatform": "java"
    },
    "groups": {
      "stable": {
        "dest": "latest",
        "moniker_range": "msal-java-latest"
      }
    },
    "versions": {
      "msal-java-latest": {
        "dest": "latest"
      }
    },    
    "markdownEngineName": "markdig",
    "fileMetadata": {
      "author": {
        "api/**/**.yml": "Dickson-Mwendia",
        "api/**/**.md": "Dickson-Mwendia"
      },
      "ms.author": {
        "api/**/**.yml": "dmwendia",
        "api/**/**.md": "dmwendia"
      },
      "ms.service": {
        "api/**/**.yml": "active-directory",
        "api/**/**.md": "active-directory"
      },
      "titleSuffix": {
        "api/**/**.md": "Microsoft Authentication Library for Java",
        "api/**/**.yml": "Microsoft Authentication Library for Java"
      },
      "ms.topic": {
        "api/**/**.md": "conceptual",
        "api/**/**.yml": "reference"
      },
      "open_to_public_contributors": {
        "api/**/**.md": true
      } 
    },
    "template": [],
    "dest": "_site"
  }
}
